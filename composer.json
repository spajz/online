{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "laravel/framework": "4.2.*",
        "way/generators": "2.*",
        "creolab/laravel-modules": "dev-master",
        "anahkiasen/former": "dev-master",
        "fzaninotto/faker": "1.5.*@dev",
        "chumper/datatable": "2.*",
        "cartalyst/sentry": "2.1.*",
        "barryvdh/laravel-ide-helper": "1.*",
        "edvinaskrucas/notification": "3.*",
        "intervention/image": "2.*",
        "barryvdh/laravel-debugbar": "1.*",
        "webpatser/laravel-sanitize": "dev-master",
        "facebook/php-sdk-v4": "4.0.*",
        "simonstamm/laravel-pjax": "dev-master",
        "cviebrock/eloquent-sluggable": "2.*",
        "baum/baum": "~1.0",
        "rtconner/laravel-tagging": "dev-master",
        "league/fractal": "0.9.*"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php",
            "app/modules",
            "app/library"
        ],
        "files": [
            "app/library/Functions.php"
        ],
        "psr-4" : {
            "Facebook\\":"vendor/facebook/php-sdk-v4/src/Facebook/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan ide-helper:generate",
            "php artisan debugbar:publish",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
